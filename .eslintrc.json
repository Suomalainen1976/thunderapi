{
  "extends": "aqua",
  "rules": {
    "quotes": [
      "warn",
      "double",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "max-len": [
      "warn",
      150
    ],
    "eol-last": 0,
    "no-process-env": "off",
    "prefer-const": "error",
    "no-var": "error"
  }
}