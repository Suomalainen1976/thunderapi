{
  "name": "thunderapi",
  "version": "0.2.0",
  "description": "Easily get data from War Thunder without having to write huge chunks of code",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && npm run lint:typings",
    "lint": "./node_modules/.bin/eslint ./src",
    "docs": "jsdoc src -r -c jsdoc.json -d ./docs -R README.md -t node_modules/minami",
    "lint:typings": "tslint ./typings/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdutchy/thunderapi.git"
  },
  "keywords": [
    "war thunder",
    "gaming",
    "node",
    "wrapper",
    "gaijin",
    "war thunder API"
  ],
  "author": "Michel Nguyen <modzongaming@gmail.com> (dutchy.ga)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/devdutchy/thunderapi/issues"
  },
  "homepage": "https://github.com/devdutchy/thunderapi#readme",
  "dependencies": {
    "common-tags": "^1.7.2",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.5.1",
    "snekfetch": "^3.6.1"
  },
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-aqua": "^2.0.1",
    "jsdoc-strip-async-await": "^0.1.0",
    "minami": "^1.2.3",
    "tslint": "^5.9.1",
    "tslint-config-typings": "^0.3.1",
    "typescript": "^2.6.2"
  }
}
